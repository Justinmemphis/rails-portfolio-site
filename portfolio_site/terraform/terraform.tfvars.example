# Example variables file for the portfolio blog Terraform module.

# Replace the following values with those from your AWS environment

aws_region       = "us-east-1"
vpc_id           = "vpc-xxxxxxxx"
subnet_id        = "subnet-xxxxxxxx"
key_name         = "portfolio-key"
public_key_path  = "~/.ssh/id_rsa.pub"
ami_id           = "ami-xxxxxxxxxxxxxxxxx" # e.g. Ubuntu 22.04 LTS AMI
instance_type    = "t3.micro"
ssh_cidr_block   = "0.0.0.0/0" # restrict to your IP for better security